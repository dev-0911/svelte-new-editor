{
    "arrowParens": "always",
    "bracketSameLine": true,
    "jsxBracketSameLine": true,
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "htmlWhitespaceSensitivity": "ignore",
    "printWidth": 180,
    "singleQuote": false,
    "singleAttributePerLine": false,
    "semi": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "plugins": ["prettier-plugin-svelte"]
}
